<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout 
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:background="#AA4AFF7F"
        android:orientation="vertical" >

	    <LinearLayout
	        android:id="@+id/dialog"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_gravity="center_horizontal"
	        android:orientation="vertical" >

	        <TextView
	            android:id="@+id/animal_info"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:text="Hola soy un okapi. Soy pariente de las jirafas. Soy vegetariano, me alimento de frutas y hojas"
	            android:layout_gravity="center_horizontal"
	            android:layout_margin="5dp"
	            android:textAppearance="?android:attr/textAppearanceMedium" />
   	        
   	        <TextView
   	            android:id="@+id/ambient_text"
   	            android:layout_width="match_parent"
   	            android:layout_height="30dp"
   	            android:layout_gravity="center_horizontal"
   	            android:background="#EE38C93B"
   	            android:text="Hábitat"
   	            android:textAppearance="?android:attr/textAppearanceLarge"
   	            android:textColor="#FFFFFF" />
	        
   	        <LinearLayout
   	            android:layout_width="match_parent"
   	            android:layout_height="wrap_content"
   	            android:background="#AA4AFF74"
   	            android:orientation="vertical" >

		       	<LinearLayout
		        	android:id="@+id/dialog1"
		        	android:layout_width="wrap_content"
		        	android:layout_height="wrap_content"
		        	android:layout_gravity="center_horizontal"
		    	    android:orientation="horizontal" >
		    	    
		       	    <LinearLayout
		        		android:id="@+id/airLayout"
		        		android:layout_width="wrap_content"
		        		android:layout_height="wrap_content"
		        		android:layout_gravity="center_horizontal"
		    	    	android:orientation="vertical" >
		    	    	
		       	    	<ImageView
		       	        	android:id="@+id/ambient_img1"
		        			android:layout_width="60dp"
		        			android:layout_height="60dp"
		        			android:layout_margin="5dp"
		        			android:background="@drawable/ambient_air"/>
	
		       	    	<TextView
		       	    	    android:id="@+id/ambient_text1"
		       	    	    android:layout_width="wrap_content"
		       	    	    android:layout_height="wrap_content"
		       	    	    android:layout_gravity="center_horizontal"
		       	    	    android:text="Aire"
		       	    	    android:textAppearance="?android:attr/textAppearanceMedium"
		       	    	    android:textColor="#000000" />
	
		       	    </LinearLayout>
		       	    
		       	    <LinearLayout
		        		android:id="@+id/treeLayout"
		        		android:layout_width="wrap_content"
		        		android:layout_height="wrap_content"
		        		android:layout_gravity="center_horizontal"
		    	    	android:orientation="vertical" >
		       	    	
			       	    <ImageView
			       	        android:id="@+id/ambient_img2"
			        		android:layout_width="60dp"
			        		android:layout_height="60dp"
			        		android:layout_margin="5dp"
			        		android:background="@drawable/ambient_tree"/>
			       	    
			       	    <TextView
			       	        android:id="@+id/ambient_text2"
			       	        android:layout_width="wrap_content"
			       	        android:layout_height="wrap_content"
			       	        android:layout_gravity="center_horizontal"
			       	        android:text="Árboles"
			       	        android:textAppearance="?android:attr/textAppearanceMedium"
			       	        android:textColor="#000000" />
			       	    
		       		</LinearLayout>
					<LinearLayout
		        		android:id="@+id/groundLayout"
		        		android:layout_width="wrap_content"
		        		android:layout_height="wrap_content"
		        		android:layout_gravity="center_horizontal"
		    	    	android:orientation="vertical" >
		    	    	
		       	    	<ImageView
		       	        	android:id="@+id/ambient_img3"
		        			android:layout_width="60dp"
		        			android:layout_height="60dp"
		        			android:layout_margin="5dp"
		        			android:background="@drawable/ambient_ground"/>
	
		       	    	<TextView
		       	    	    android:id="@+id/ambient_text3"
		       	    	    android:layout_width="wrap_content"
		       	    	    android:layout_height="wrap_content"
		       	    	    android:layout_gravity="center_horizontal"
		       	    	    android:text="Tierra"
		       	    	    android:textAppearance="?android:attr/textAppearanceMedium"
		       	    	    android:textColor="#000000" />
	
		       	    </LinearLayout>
		       	    <LinearLayout
		        		android:id="@+id/riverlayout"
		        		android:layout_width="wrap_content"
		        		android:layout_height="wrap_content"
		        		android:layout_gravity="center_horizontal"
		    	    	android:orientation="vertical" >
		    	    	
		       	    	<ImageView
		       	        	android:id="@+id/ambient_img4"
		        			android:layout_width="60dp"
		        			android:layout_height="60dp"
		        			android:layout_margin="5dp"
		        			android:background="@drawable/ambient_river"/>
	
		       	    	<TextView
		       	    	    android:id="@+id/ambient_text4"
		       	    	    android:layout_width="wrap_content"
		       	    	    android:layout_height="wrap_content"
		       	    	    android:layout_gravity="center_horizontal"
		       	    	    android:text="Río"
		       	    	    android:textAppearance="?android:attr/textAppearanceMedium"
		       	    	    android:textColor="#000000" />
	
		       	    </LinearLayout>
		       		    
				</LinearLayout>
   	        </LinearLayout>

   	        <TextView
   	            android:id="@+id/food_text"
   	            android:layout_width="match_parent"
   	            android:layout_height="30dp"
   	            android:layout_gravity="center_horizontal"
   	            android:background="#EE38C93B"
   	            android:text="Comida"
   	            android:textAppearance="?android:attr/textAppearanceLarge"
   	            android:textColor="#FFFFFF" />

   	        <LinearLayout
   	            android:layout_width="match_parent"
   	            android:layout_height="wrap_content"
   	            android:background="#AA4AFF74"
   	            android:orientation="vertical" >

		       	<LinearLayout
		        	android:id="@+id/dialog2"
		        	android:layout_width="wrap_content"
		        	android:layout_height="wrap_content"
		        	android:layout_gravity="center_horizontal"
		    	    android:orientation="horizontal" >
		    	    
		       	    <LinearLayout
		        		android:id="@+id/fruitLayout"
		        		android:layout_width="wrap_content"
		        		android:layout_height="wrap_content"
		        		android:layout_gravity="center_horizontal"
		    	    	android:orientation="vertical" >
			       	    
			       	    <ImageView
			       	        android:id="@+id/food_img1"
			        		android:layout_width="60dp"
			        		android:layout_height="60dp"
			        		android:layout_margin="5dp"
			        		android:background="@drawable/food_fruit_info"/>
			       	    
			       	    <TextView
			       	        android:id="@+id/ambient_text2"
			       	        android:layout_width="wrap_content"
			       	        android:layout_height="wrap_content"
			       	        android:layout_gravity="center_horizontal"
			       	        android:text="Frutas"
			       	        android:textAppearance="?android:attr/textAppearanceMedium"
			       	        android:textColor="#000000" />
			       	    
			       	</LinearLayout>
		       	    
			       	<LinearLayout
		        		android:id="@+id/insectLayout"
		        		android:layout_width="wrap_content"
		        		android:layout_height="wrap_content"
		        		android:layout_gravity="center_horizontal"
		    	    	android:orientation="vertical" >
			       	    
			       	    <ImageView
			       	        android:id="@+id/food_img2"
			        		android:layout_width="60dp"
			        		android:layout_height="60dp"
			        		android:layout_margin="5dp"
			        		android:background="@drawable/food_insect_info"/>
	   		       	    
	   		       	    <TextView
			       	        android:id="@+id/ambient_text2"
			       	        android:layout_width="wrap_content"
			       	        android:layout_height="wrap_content"
			       	        android:layout_gravity="center_horizontal"
			       	        android:text="Insectos"
			       	        android:textAppearance="?android:attr/textAppearanceMedium"
			       	        android:textColor="#000000" />
			       	    
		       		</LinearLayout>
		       		<LinearLayout
		        		android:id="@+id/plantLayout"
		        		android:layout_width="wrap_content"
		        		android:layout_height="wrap_content"
		        		android:layout_gravity="center_horizontal"
		    	    	android:orientation="vertical" >
			       	    
			       	    <ImageView
			       	        android:id="@+id/food_img3"
			        		android:layout_width="60dp"
			        		android:layout_height="60dp"
			        		android:layout_margin="5dp"
			        		android:background="@drawable/food_plant_info"/>
	   		       	    
	   		       	    <TextView
			       	        android:id="@+id/food_text3"
			       	        android:layout_width="wrap_content"
			       	        android:layout_height="wrap_content"
			       	        android:layout_gravity="center_horizontal"
			       	        android:text="Plantas"
			       	        android:textAppearance="?android:attr/textAppearanceMedium"
			       	        android:textColor="#000000" />
			       	    
		       		</LinearLayout>
					<LinearLayout
		        		android:id="@+id/meatLayout"
		        		android:layout_width="wrap_content"
		        		android:layout_height="wrap_content"
		        		android:layout_gravity="center_horizontal"
		    	    	android:orientation="vertical" >
			       	    
			       	    <ImageView
			       	        android:id="@+id/food_img4"
			        		android:layout_width="60dp"
			        		android:layout_height="60dp"
			        		android:layout_margin="5dp"
			        		android:background="@drawable/food_meat_info"/>
	   		       	    
	   		       	    <TextView
			       	        android:id="@+id/food_text4"
			       	        android:layout_width="wrap_content"
			       	        android:layout_height="wrap_content"
			       	        android:layout_gravity="center_horizontal"
			       	        android:text="Carne"
			       	        android:textAppearance="?android:attr/textAppearanceMedium"
			       	        android:textColor="#000000" />
			       	    
		       		</LinearLayout>
		       		    
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>

		<Button
		    android:id="@+id/dialogButtonOK"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_gravity="center_horizontal"
		    android:text=" Ok " />

	</LinearLayout>
    
    
</RelativeLayout>